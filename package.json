{
  "name": "terrafusion",
  "version": "1.0.0",
  "description": "TerraFusion - Advanced Geospatial Intelligence Platform",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "npm run copy:shared && vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --inject:server/esm-shim.js",
    "build:production": "npm run build && npm run copy:shared",
    "copy:shared": "cp -r shared dist/ || xcopy shared dist\\shared /E /I /Y",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npx tsc --noEmit",
    "db:push": "drizzle-kit push",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npx tsc --noEmit --watch",
    "quality:check": "npm run type-check && npm run lint && npm run format:check && npx cspell \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "quality:fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "pre-commit": "npm run quality:fix && npm run type-check",
    "production:check": "npm run quality:check && npm run test && npm run build",
    "deploy:enterprise": "chmod +x deploy-enterprise.sh && ./deploy-enterprise.sh",
    "deploy:quick": "npm run build:production && node dist/index.js",
    "cleanup": "npm run quality:fix && npm prune && npm audit fix"
  },
