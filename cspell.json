{
  "version": "0.2",
  "language": "en",
  "words": [
    "TerraFusion",
    "terrafusion",
    "terrafield",
    "ICSF",
    "PostgreSQL",
    "postgres",
    "postgis",
    "WebSocket",
    "websocket",
    "socketio",
    "TypeScript",
    "nodejs",
    "QGIS",
    "qgis",
    "PostGIS",
    "geojson",
    "mapboxgl",
    "shapefile",
    "Shapefile",
    "multipolygon",
    "choropleth",
    "Choropleth",
    "hillshade",
    "hillshading",
    "Hillshade",
    "Hillshading",
    "viewshed",
    "Viewshed",
    "geoprocessing",
    "basemap",
    "USGS",
    "usgs",
    "mapzen",
    "CAMA",
    "cama",
    "assessor",
    "taxlot",
    "Comparables",
    "comparables",
    "BCBS",
    "TTFB",
    "codeagent",
    "plandex",
    "Plandex",
    "openai",
    "anthropic",
    "middleware",
    "uuid",
    "apikey",
    "hoverable",
    "rendercomplete",
    "moveend",
    "notif",
    "nums",
    "topo"
  ],
  "ignorePaths": [
    "node_modules/**",
    "dist/**",
    "build/**",
    ".git/**",
    "*.log",
    "*.lock",
    "package-lock.json",
    "yarn.lock",
    ".vscode/**",
    ".idea/**",
    "coverage/**",
    ".nyc_output/**",
    "*.min.js",
    "*.bundle.js",
    "*.map",
    "*.tsbuildinfo",
    "archive/**"
  ],
  "overrides": [
    {
      "filename": "**/*.{ts,tsx,js,jsx,rs}",
      "caseSensitive": false
    },
    {
      "filename": "**/*.md",
      "language": "en",
      "caseSensitive": false
    },
    {
      "filename": "**/*.json",
      "caseSensitive": false
    }
  ],
  "flagWords": ["hte", "teh", "recieve", "seperate", "occured"],
  "patterns": [
    {
      "name": "hex-colors",
      "pattern": "#[0-9a-fA-F]{3,8}\\b"
    },
    {
      "name": "version-numbers",
      "pattern": "\\d+\\.\\d+\\.\\d+(-\\w+)?"
    },
    {
      "name": "api-endpoints",
      "pattern": "/api/[a-zA-Z0-9/-]+"
    },
    {
      "name": "tech-terms",
      "pattern": "\\b(React|AWS|GIS|JWT|OAuth|CORS|HTTPS|Redis|Docker|DevOps|vite|eslint|drizzle|express)\\b"
    },
    {
      "name": "file-extensions",
      "pattern": "\\.[a-zA-Z0-9]{2,4}\\b"
    }
  ]
}
